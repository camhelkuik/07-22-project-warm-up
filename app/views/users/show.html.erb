<div class= "nav">
  <ul>
    <li><a href="<%= profile_path %>">My Profile</a></li>
    <li><a href="<%= profile_edit_path %>">Edit User Info</a></li>
    <li><a href="<%= user_path(@user.id) %>" data-method="delete">Delete User Profile</a></li>
    <li><a href="<%= logout_path %>">LOGOUT</a></li>
  </ul>
</div>

<div class= "user_name">
  <h1><%= @user.email %></h1>
  <a href="<%= new_task_path %>">Add new task</a>
</div>

<div>
  
 
  <% @task.each do |t| %>
  <div class= "task_list">
    <p><%= t.description %></p>
  
    <a href="<%= task_path(t.id) %>" data-method="delete">Delete</a>
    <a href="<%= edit_task_path(t.id) %>">Edit</a>
  </div>
  <% end %>
 
</div>